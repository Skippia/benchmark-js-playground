{
  "name": "benchmark-js-playground",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "keywords": [],
  "main": "index.js",
  "scripts": {
    "eslint": "npx eslint .",
    "eslint:fix": "npx eslint . --fix",
    "clear:benchmarks": "find benchmarks-data/ -maxdepth 1 -type f -name 'benchmark-*.json' -delete",
    "----------------------------------------------": "",
    "docker:infra": "docker-compose -f ./src/infra/docker-compose.yaml down && docker-compose -f ./src/infra/docker-compose.yaml up",
    "docker:clear": "docker volume rm $(docker volume ls -f dangling=true -q) && docker rm -f $(docker ps -a --filter name=^nmt- -q) && docker rmi $(docker images -f dangling=true -q)",
    "-----------------------------------------------": "",
    "server": "NODE_ENV=production node --env-file=.env ./dist/server/entrypoint.js",
    "benchmark:automate": "NODE_ENV=production node --env-file=.env ./dist/benchmarks/automate.js -automate",
    "benchmark:manual": "NODE_ENV=production node --env-file=.env ./dist/benchmarks/benchmark.js",
    "------------------------------------------------": "",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "-------------------------------------------------": "",
    "build": "rm -rf dist && npx tsc --noEmit false && tsc-alias",
    "build:watch": "concurrently \"npx tsc -w --noEmit false\" \"tsc-alias -w\""
  },
  "dependencies": {
    "express": "^4.19.2",
    "fastify": "^4.28.1",
    "ioredis": "^5.4.1",
    "pg": "^8.12.0",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.48.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.0.0",
    "@types/autocannon": "^7.12.5",
    "@types/bun": "^1.1.8",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.4",
    "autocannon": "^7.15.0",
    "concurrently": "^9.0.1",
    "eslint": "^9.9.1",
    "globals": "^15.9.0",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4"
  }
}
